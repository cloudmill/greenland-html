section.section.projects(data-scroll-section="" data-transparent-end='')
  .container
    .projects__header
      .projects__title.title-h2(data-aos="fade-up") Наши проекты

      .projects__col
        h3.projects__header-heading Green Land — #[span.accent.text--lowercase #[span.projects__highlight.projects__highlight--1 эксперты]] #[span.txt-desktop в дизайнерском #[br] озеленении] #[span.txt-mobile в дизайнерском озеленении] #[span.accent.text--lowercase #[span.projects__highlight.projects__highlight--2 более 15 лет]]

      .projects__col
        .accent.text--none.projects__header-subheading.only-desktop Трансформируем общественные места #[br]в приятные, гармоничные, безопасные.
        .accent.text--none.projects__header-subheading.only-mobile Трансформируем общественные места в#[br] приятные, гармоничные, безопасные.
        a.btn.projects__header-btn(href="#") каталог продукции

    .projects__slider.swiper-container(data-projects-slider="" data-catalog='')
      .swiper-wrapper
        .swiper-slide.projects__slide
          .projects__item
            .projects__item-slider(data-catalog-card='')
              .projects__item-slider-wrapper(data-fade-item-slider-wrapper="")
                ul.catalog-list
                  each item in ["assets/images/projects__item-img-3.jpg", "https://via.placeholder.com/600/771796", "https://via.placeholder.com/150/92c952", "https://via.placeholder.com/150/1fe46f"]
                    li.catalog-list__item(data-catalog-image = item)
                img.projects__item-img(src="assets/images/projects__item-img-3.jpg", alt="" data-card-image='')
              .projects__item-bullets 
                .catalog-pagination
                  - for (x = 0; x < 4; x++)
                    .catalog-pagination__item
            .projects__item-caption
              .epsilon.projects__item-name Отель Crowne Plaza
              .projects__item-tags
                .projects__item-tag.text__super-tn.text--uppercase Вертикальное озеленение
                .projects__item-tag.text__super-tn.text--uppercase Террасы
                .projects__item-tag.text__super-tn.text--uppercase Входные группы
        .swiper-slide.projects__slide
          .projects__item
            .projects__item-slider(data-catalog-card='')
              .projects__item-slider-wrapper(data-fade-item-slider-wrapper="")
                ul.catalog-list
                  each item in ["assets/images/projects__item-img-1.jpg", "https://via.placeholder.com/600/771796", "https://via.placeholder.com/150/92c952"]
                    li.catalog-list__item(data-catalog-image = item)
                img.projects__item-img(src="assets/images/projects__item-img-1.jpg", alt="" data-card-image='')
              .projects__item-bullets 
                .catalog-pagination
                  - for (x = 0; x < 3; x++)
                    .catalog-pagination__item
            .projects__item-caption
              .epsilon.projects__item-name Отель Crowne Plaza
              .projects__item-tags
                .projects__item-tag.text__super-tn.text--uppercase Вертикальное озеленение
                .projects__item-tag.text__super-tn.text--uppercase Террасы
                .projects__item-tag.text__super-tn.text--uppercase Входные группы
        .swiper-slide.projects__slide
          .projects__item
            .projects__item-slider(data-catalog-card='')
              .projects__item-slider-wrapper(data-fade-item-slider-wrapper="")
                ul.catalog-list
                  each item in ["assets/images/projects__item-img-2.jpg", "https://via.placeholder.com/600/771796"]
                    li.catalog-list__item(data-catalog-image = item)
                img.projects__item-img(src="assets/images/projects__item-img-2.jpg", alt="" data-card-image='')
              .projects__item-bullets 
                .catalog-pagination
                  - for (x = 0; x < 2; x++)
                    .catalog-pagination__item
            .projects__item-caption
              .epsilon.projects__item-name Отель Crowne Plaza
              .projects__item-tags
                .projects__item-tag.text__super-tn.text--uppercase Вертикальное озеленение
                .projects__item-tag.text__super-tn.text--uppercase Террасы
                .projects__item-tag.text__super-tn.text--uppercase Входные группы
      .projects__slider-controls
        .projects-controls-container
          +arrow('#0F2C2D', '#CAE8D9', 'left', 18, 14, 50)(data-projects-slider-prev="")
        .projects-controls-container
          +arrow('#0F2C2D', '#CAE8D9', 'right', 18, 14, 50)(data-projects-slider-next="")